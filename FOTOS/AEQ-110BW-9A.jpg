		function applyOffset(selector, headerSelector){
            // Get nav height on load
			const head_height = $(headerSelector).outerHeight();
			//console.log(headerSelector);console.log(head_height);
			$(selector).css("padding-top", head_height); 			
        }
    
    	function applyScroll(headerSelector) {
    		let didScroll;
    	    let lastScrollTop = 0;
    	    const delta = 30;
	    	const head_height = $(headerSelector).outerHeight();
    		
			window.addEventListener("scroll", function(event){
                didScroll = true;
            });
    	
    	    setInterval(function() {
    	        if (didScroll) {
    	            hasScrolled();
    	            didScroll = false;
    	        }
    	    }, 250);
    	
    	    function hasScrolled() {
    	        const st = window.pageYOffset;
    	        
    	        // Make sure they scroll more than delta
    	        if(Math.abs(lastScrollTop - st) <= delta) {
    	            return;
    	        }
    	        // If they scrolled down and are past the navbar, add class .move-up.
    	        if (st > lastScrollTop && st > head_height){
    	            $(headerSelector).addClass('compress');   
    	        } else {
    	            // Scroll Up
    	            if(st + $(window).height() < $(document).height()) {
    	                $(headerSelector).removeClass('compress');
    	            }
    	        }
    	
    	        lastScrollTop = st;
    	    }
    	}
    	
    	function applyMarqueeOnOverflow() {
    		const markeeOverflows = document.getElementsByClassName("markee-overflow-container");
        	Array.from(markeeOverflows).forEach((el) => {
                $(el).removeClass('marquee');
                
                if(isElementOverflowing(el)) {
                	$(el).addClass('marquee');
                } 
            });
        }
        
        
    	$(function () {
			//This is the first applyOffset. Maybe its out of timing so it needs to be executed again in window.'load' event. This is being executed only because its early
			applyOffset("body", '#mainHeader-wrapper');

			//
        	$(window).resize(function() {
                applyOffset("body", '#mainHeader-wrapper');
                
                //marquee
                applyMarqueeOnOverflow();
            });

        	//marquee
        	applyMarqueeOnOverflow();

			            	// Apply Scroll action on load
            	applyScroll('#mainHeader-wrapper');
	    	    	});

		window.addEventListener("load", function () {
			// Apply offset nav height on load and on resize
			applyOffset("body", '#mainHeader-wrapper');
		});
	</script>

		
